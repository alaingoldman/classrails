<h2> edit page </h2>

<%= form_for @micropost do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<br />
	<%= f.label :email %>
	<%= f.text_field :email %>
	<br />
	<%= f.label :message %>
	<%= f.text_field :message %>
	<br />
	<%= f.submit "submit", :class => "btn btn-success" %>
<% end %>
